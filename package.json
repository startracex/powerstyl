{
  "name": "powerstyl",
  "description": "A way to style elements in vanilla JavaScript",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "unnestcss": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rollup": "^4.46.4",
    "rollup-plugin-oxc": "^0.6.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./package.json": "./package.json",
    "./src/*": "./src/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "powerstyl",
    "css",
    "css-in-js",
    "styled"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/powerstyl.git"
  },
  "author": "startracex",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0"
}
